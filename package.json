{
  "name": "fbtools",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.9"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "axios": "^0.21.1",
    "draft-components": "0.26.0",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.10.13",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-styleguidist": "^11.1.5",
    "react-transition-group": "^4.4.1",
    "sass": "^1.30.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "formatjs:extract": "formatjs extract 'src/**/*.ts*' --ignore 'src/**/*.d.ts' --out-file src/translations/messages.json",
    "formatjs:compile": "formatjs compile src/translations/messages.json --out-file src/translations/en-US.json",
    "translations": "npm run formatjs:extract && npm run formatjs:compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/no-offset": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "3.0.5",
    "eslint-plugin-formatjs": "^2.10.3",
    "prettier": "^2.2.1"
  }
}
